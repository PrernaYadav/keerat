<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:orientation="vertical"
    tools:context="com.example.user10.keerat.activity.activity.Registerserviceoptionsenabled">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <EditText
                android:id="@+id/et_enterfullname"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="@string/enterfullname"
                android:inputType="text"
                android:isScrollContainer="false"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" />




            <TextView
                android:id="@+id/et_enterdob"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="@string/enterdatebirth"
                android:onClick="showDatePickerDialog"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_gender"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:text="Gender"
                    android:textStyle="bold"
                    android:textColor="@android:color/white"
                    android:textSize="14sp" />

                <RadioGroup
                    android:id="@+id/rg_gender"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="85dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:text="Male"
                        android:textColor="@android:color/white"
                        android:textSize="14sp" />

                    <RadioButton
                        android:id="@+id/rb_male"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:buttonTint="@android:color/white"
                        android:checked="false" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:text="Female"
                        android:textColor="@android:color/white"
                        android:textSize="14sp" />

                    <RadioButton
                        android:id="@+id/rb_female"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:buttonTint="@android:color/white"
                        android:checked="false" />
                </RadioGroup>
            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_perform"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:textStyle="bold"
                    android:text="@string/performingroup"
                    android:textColor="@android:color/white"
                    android:textSize="14sp" />

                <RadioGroup
                    android:id="@+id/rg_performgroup"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:text="@string/yes"
                        android:textColor="@android:color/white"
                        android:textSize="14sp" />

                    <RadioButton
                        android:id="@+id/rb_yes1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:buttonTint="@android:color/white"
                        android:checked="false" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="4dp"
                        android:text="@string/no"
                        android:textColor="@android:color/white"
                        android:textSize="14sp" />

                    <RadioButton
                        android:id="@+id/rb_no1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:buttonTint="@android:color/white"
                        android:checked="false" />
                </RadioGroup>
            </LinearLayout>

            <EditText
                android:id="@+id/et_performers"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="@string/performer"
                android:inputType="text"
                android:isScrollContainer="false"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp"
                android:visibility="gone" />

            <EditText
                android:id="@+id/et_jatha"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="@string/jathaname"
                android:inputType="text"
                android:isScrollContainer="false"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/et_altermobile"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="@string/altermobile"
                android:inputType="number"
                android:isScrollContainer="false"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" />




               <AutoCompleteTextView
                android:id="@+id/auto_country"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="Country"
                android:padding="15dp"
                android:textColor="@android:color/white"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" >
                   <requestFocus />
            </AutoCompleteTextView>


            <AutoCompleteTextView
                android:id="@+id/auto_state"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="State"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColor="@android:color/white"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" >
            <requestFocus />
            </AutoCompleteTextView>
            <AutoCompleteTextView
                android:id="@+id/auto_city"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:textColor="@android:color/white"
                android:background="@drawable/roundedet_border"
                android:hint="City"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" >
            <requestFocus />
            </AutoCompleteTextView>


            <EditText
                android:id="@+id/et_address"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="House no/Street/society"
                android:inputType="text"
                android:isScrollContainer="false"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" />
            <EditText
                android:id="@+id/et_addres2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="Area Name/Pin code"
                android:inputType="text"
                android:isScrollContainer="false"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" />


            <EditText
                android:id="@+id/et_pincode"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="@string/pin"
                android:inputType="number"
                android:isScrollContainer="false"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" />

            <Spinner
                android:id="@+id/sp_more"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/drop_down_style"
                android:entries="@array/more_array"
                android:padding="16dp"
                android:paddingLeft="22dp"
                android:paddingRight="22dp"
                android:prompt="@string/select">

            </Spinner>

            <EditText
                android:id="@+id/et_performduration"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:visibility="gone"
                android:background="@drawable/roundedet_border"
                android:hint="@string/performduration"
                android:inputType="number"
                android:isScrollContainer="false"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" />

            <Spinner
                android:id="@+id/sp_category"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/drop_down_style"
                android:entries="@array/categor_array"
                android:padding="16dp"
                android:paddingLeft="22dp"
                android:paddingRight="22dp"
                android:prompt="@string/select">
            </Spinner>

            <Spinner
                android:id="@+id/sp_genre"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/drop_down_style"
                android:entries="@array/genre_array"
                android:padding="16dp"
                android:paddingLeft="22dp"
                android:paddingRight="22dp"
                android:prompt="@string/select">
            </Spinner>

            <Spinner
                android:id="@+id/sp_subgenre"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/drop_down_style"
                android:entries="@array/genre_array"
                android:padding="16dp"
                android:paddingLeft="22dp"
                android:paddingRight="22dp"
                android:prompt="@string/select">
            </Spinner>



            <EditText
                android:id="@+id/et_specify"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="Others(Please specify)"
                android:inputType="text"
                android:visibility="gone"
                android:isScrollContainer="false"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" />


            <EditText
                android:id="@+id/et_tagline"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="@string/tagline"
                android:inputType="text"
                android:isScrollContainer="false"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" />


            <TextView
                android:id="@+id/tv_about"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:text="@string/about"
                android:textColor="@android:color/white"
                android:textSize="14sp" />

            <EditText
                android:id="@+id/et_abturself"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="5dp"
                android:background="@drawable/roundedet_border"
                android:hint="@string/tellus"
                android:inputType="textMultiLine"
                android:isScrollContainer="false"
                android:maxLines="3"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" />

            <Button
                android:id="@+id/btn_uploadpic"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/rounded_edittext"
                android:padding="15dp"
                android:text="@string/uploadpic"
                android:textColor="#fe7a47"
                android:textSize="14sp" />

         <!--   <Button
                android:id="@+id/btn_uploadvid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/rounded_edittext"
                android:padding="15dp"
                android:text="@string/uploadvideo"
                android:textColor="#fe7a47"
                android:textSize="14sp" />-->

            <Spinner
                android:id="@+id/sp_serve"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/drop_down_style"
                android:entries="@array/serve_array"
                android:padding="16dp"
                android:paddingLeft="22dp"
                android:paddingRight="22dp"
                android:prompt="@string/select">

            </Spinner>


            <EditText
                android:id="@+id/et_otherinfo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="@string/otherinfo"
                android:inputType="text"
                android:isScrollContainer="false"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp" />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="20dp"
                android:orientation="horizontal"
                android:weightSum="2">

                <TextView
                    android:id="@+id/tv_behtarahat"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:text="@string/bahtarahat"
                    android:textColor="@android:color/white"
                    android:textSize="14sp" />

                <RadioGroup
                    android:id="@+id/rg_bhetarahat"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginLeft="25dp"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="@string/yes"
                        android:textColor="@android:color/white"

                        android:textSize="14sp" />

                    <RadioButton
                        android:id="@+id/rb_yes3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:buttonTint="@android:color/white"
                        android:checked="false" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:text="@string/no"
                        android:textColor="@android:color/white"
                        android:textSize="14sp" />

                    <RadioButton
                        android:id="@+id/rb_no3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:buttonTint="@android:color/white"
                        android:checked="false" />
                </RadioGroup>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/ll_bhetarahet"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">


                <EditText
                    android:id="@+id/et_expectedprice"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginTop="5dp"
                    android:background="@drawable/roundedet_border"
                    android:hint="@string/PricePerProgramme"
                    android:inputType="textMultiLine"
                    android:isScrollContainer="false"
                    android:maxLines="3"
                    android:padding="15dp"
                    android:paddingLeft="22dp"
                    android:textColorHint="@color/hintcolor"
                    android:textSize="14sp" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginTop="20dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <TextView
                        android:id="@+id/tv_donotdisplay"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:maxLines="2"
                        android:paddingBottom="15dp"
                        android:paddingTop="15dp"
                        android:text="@string/donotdisplay"
                        android:textColor="@android:color/white"
                        android:textSize="14sp" />

                    <RadioGroup
                        android:id="@+id/rg_donotdisplay"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="@string/yes"
                            android:textColor="@android:color/white"
                            android:textSize="14sp" />

                        <RadioButton
                            android:id="@+id/rb_yes4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:buttonTint="@android:color/white"
                            android:checked="false" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:text="@string/no"
                            android:textColor="@android:color/white"
                            android:textSize="14sp" />

                        <RadioButton
                            android:id="@+id/rb_no4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:buttonTint="@android:color/white"
                            android:checked="false" />
                    </RadioGroup>
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/ll_dynamicprice"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginTop="20dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <TextView
                        android:id="@+id/tv_dynamicprice"
                        android:layout_width="140dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:maxLines="2"
                        android:paddingBottom="15dp"
                        android:paddingTop="15dp"
                        android:text="@string/dynamicprice"
                        android:textColor="@android:color/white"
                        android:textSize="14sp" />

                    <RadioGroup
                        android:id="@+id/rg_dynamicprice"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="@string/yes"
                            android:textColor="@android:color/white"
                            android:textSize="14sp" />

                        <RadioButton
                            android:id="@+id/rb_yes5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:buttonTint="@android:color/white"
                            android:checked="false" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="10dp"
                            android:text="@string/no"
                            android:textColor="@android:color/white"
                            android:textSize="14sp" />

                        <RadioButton
                            android:id="@+id/rb_no5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:buttonTint="@android:color/white"
                            android:checked="false" />
                    </RadioGroup>
                </LinearLayout>

                <EditText
                    android:id="@+id/et_enterprice"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="30dp"
                    android:layout_marginTop="31dp"
                    android:background="@drawable/roundedet_border"
                    android:hint="Enter Price/City"
                    android:textColor="@android:color/white"
                    android:inputType="text"
                    android:isScrollContainer="false"
                    android:padding="15dp"
                    android:paddingLeft="22dp"
                    android:textColorHint="@color/hintcolor"
                    android:textSize="14sp"
                    android:visibility="gone" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="20dp"
                android:orientation="horizontal"
                android:weightSum="2">

                <TextView
                    android:id="@+id/tv_addcharge"
                    android:layout_width="140dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:maxLines="2"
                    android:paddingBottom="15dp"
                    android:paddingTop="15dp"
                    android:text="@string/addcharge"
                    android:textColor="@android:color/white"
                    android:textSize="14sp" />

                <RadioGroup
                    android:id="@+id/rg_charge"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text="@string/yes"
                        android:textColor="@android:color/white"
                        android:textSize="14sp" />

                    <RadioButton
                        android:id="@+id/rb_yes2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:buttonTint="@android:color/white"
                        android:checked="false" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:text="@string/no"
                        android:textColor="@android:color/white"
                        android:textSize="14sp" />

                    <RadioButton
                        android:id="@+id/rb_no2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:buttonTint="@android:color/white"
                        android:checked="false" />
                </RadioGroup>
            </LinearLayout>


            <EditText
                android:id="@+id/et_charge"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="31dp"
                android:background="@drawable/roundedet_border"
                android:hint="Enter Coneyance Charge"
                android:inputType="text"
                android:textColor="@android:color/white"
                android:isScrollContainer="false"
                android:padding="15dp"
                android:paddingLeft="22dp"
                android:textColorHint="@color/hintcolor"
                android:textSize="14sp"
                android:visibility="gone" />



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:layout_marginTop="15dp"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/cb"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:backgroundTint="@android:color/white"
                    android:buttonTint="@android:color/white"
                    android:checked="false" />


                <TextView
                    android:id="@+id/tv_termsss"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="1dp"
                    android:text="Terms and Conditions"
                    android:textColor="@android:color/white"
                    android:textColorHint="@color/hintcolor"
                    android:textSize="14sp" />

            </LinearLayout>

            <Button
                android:id="@+id/btn_serviceenable_submit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="24dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/button_style"
                android:ems="8"
                android:gravity="center"
                android:padding="15dp"
                android:text="@string/submit"
                android:textColor="@android:color/white"
                android:textSize="14sp"
                android:textStyle="bold" />


        </LinearLayout>
    </ScrollView>


</LinearLayout>
